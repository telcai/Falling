
<article class="<%= item.layout %>">
	<div class="article-img">
		<% if (item.photos.length){ %>
    		<%- partial('post/gallery') %>
  		<% }else{ %>
  		    <%- partial('post/gallery2') %>
  		<% } %>

  		<div class="art-declare">
  			<h4><a href="<%- config.root %><%- item.path %>" title="<%= item.title %>"><%= item.title %></a></h4>
  			<div class="post-tag"><%- partial('post/tag') %></div>
  		</div>
  	</div>
  	
</article>